            });
            $(document).ready(function(){
                $('.new-p-heading li').click(function(){
                    const value = $(this).attr('data-filter');
                    if(value == 'all'){
                        $('.new-product-box').show('1000');
                    }
                    else{
                        $('.new-product-box').not('.'+value).hide('1000');
                        $('.new-product-box').filter('.'+value).show('1000');
                    }
                });
            });
